{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@layouts/*": ["src/layouts/*"],
			"@components/*": ["src/components/*"],
			"@css/*": ["src/css/*"],
			"@utils/*": ["src/utils/*"],
			"@images/*": ["src/images/*"]
		}
	}
}
